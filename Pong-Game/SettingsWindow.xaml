<Window x:Class="Pong_Game.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pong_Game"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="windowSetting" Background="Black">
            <Button x:Name="BackToStart" Width="100" Height="35" Content="Back to the Start" Background="DarkRed" Foreground="White" FontFamily="Arial"/>

        </Canvas>
    </Grid>
</Window>
